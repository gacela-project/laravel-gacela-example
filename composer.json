{
    "name": "gacela-project/laravel-gacela-example",
    "type": "project",
    "description": "Laravel using Gacela Framework",
    "keywords": ["framework", "gacela", "laravel"],
    "require": {
        "php": "^7.4|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "gacela-project/gacela": "^0.8",
        "guzzlehttp/guzzle": "^7.3",
        "laravel/framework": "^8.60",
        "laravel/tinker": "^2.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "facade/ignition": "^2.12",
        "fakerphp/faker": "^1.16",
        "laravel/sail": "^1.10",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5.9",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
