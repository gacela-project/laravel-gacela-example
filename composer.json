{
    "name": "gacela-project/laravel-gacela-example",
    "type": "project",
    "description": "Laravel using Gacela Framework",
    "keywords": ["framework", "gacela", "laravel"],
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^9.14",
        "laravel/sanctum": "^2.15.1",
        "laravel/tinker": "^2.7",
        "gacela-project/gacela": "^0.25",
        "gacela-project/gacela-env-config-reader": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.19.0",
        "laravel/sail": "^1.14.7",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.2",
        "phpunit/phpunit": "^9.5.20",
        "spatie/laravel-ignition": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
